{
  "name": "kairosdb-webui",
  "description": "Angular 2 rewrite of the KairosDB query builder WebUI",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js && node copy-dist-files.js",
    "lite:aot": "lite-server -c aot/bs-config.json"
  },
  "license": "ISC",
